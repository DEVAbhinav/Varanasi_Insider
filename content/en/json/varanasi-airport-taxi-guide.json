    {
      "@type": "HowTo",
      "name": "How to book a prepaid taxi at Varanasi airport",
      "image": "/images/airport-taxi-600x400.jpeg",
      "totalTime": "PT5M",
      "step": [
        {
          "@type": "HowToStep",
          "position": 1,
          "text": "Find the Prepaid Taxi counter inside arrivals hall, left of the exit."
        },
        {
          "@type": "HowToStep",
          "position": 2,
          "text": "Tell the officer your hotel or ghat."
        },
        {
          "@type": "HowToStep",
          "position": 3,
          "text": "Pay the fixed fare (cash, UPI or card; carry backup cash)."
        },
        {
          "@type": "HowToStep",
          "position": 4,
          "text": "Collect the printed receipt—this is your contract."
        },
        {
          "@type": "HowToStep",
          "position": 5,
          "text": "Walk to Bays 5–8, show the slip to the coordinator and board your taxi."
        }
      ]
    },
    {
      "@type": "FAQPage",
      "mainEntity": [
        {
          "@type": "Question",
          "name": "Is there a night surcharge for prepaid taxis?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "No. The amount on your prepaid slip is all‑inclusive. App‑cab fares, however, may surge at night."
          }
        },
        {
          "@type": "Question",
          "name": "Can I pay with UPI or card at the prepaid counter?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Usually yes, but connectivity sometimes fails; carry some cash as a fallback."
          }
        },
        {
          "@type": "Question",
          "name": "Which is better at the airport, Uber or Ola?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Check both apps on arrival; supply and surge differ hour‑to‑hour."
          }
        },
        {
          "@type": "Question",
          "name": "How far is the airport from the main ghats?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Approximately 25 km (22–25 km by route), taking 45–90 minutes depending on traffic."
          }
        },
        {
          "@type": "Question",
          "name": "What to do if I leave belongings in a cab?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "For prepaid, call the airport police with your slip number. For app‑cabs, use the in‑app ‘lost item’ feature."
          }
        }
      ]
    },
